<svg width="200" height="200" viewBox="0 0 200 200" xmlns="http://www.w3.org/2000/svg">
  <!-- Background Circle -->
  <circle cx="100" cy="100" r="95" fill="#dc2626" opacity="0.1"/>
  <circle cx="100" cy="100" r="95" fill="none" stroke="#dc2626" stroke-width="3"/>

  <!-- Truck Icon -->
  <g transform="translate(50, 70)">
    <!-- Truck body -->
    <rect x="20" y="10" width="60" height="35" fill="#dc2626" rx="3"/>
    <!-- Truck cab -->
    <rect x="5" y="20" width="20" height="25" fill="#dc2626" rx="3"/>
    <!-- Truck window -->
    <rect x="8" y="23" width="14" height="12" fill="#fee2e2"/>

    <!-- Wheels -->
    <circle cx="30" cy="50" r="8" fill="#374151"/>
    <circle cx="30" cy="50" r="4" fill="#9ca3af"/>
    <circle cx="65" cy="50" r="8" fill="#374151"/>
    <circle cx="65" cy="50" r="4" fill="#9ca3af"/>
  </g>

  <!-- Check Mark -->
  <g transform="translate(115, 90)">
    <circle cx="0" cy="0" r="22" fill="#00a651"/>
    <path d="M -8 -2 L -3 5 L 10 -10" fill="none" stroke="white" stroke-width="4" stroke-linecap="round" stroke-linejoin="round"/>
  </g>

  <!-- Clean Air Waves -->
  <g opacity="0.3">
    <path d="M 60 100 Q 70 95, 80 100" fill="none" stroke="#00a651" stroke-width="2" stroke-linecap="round"/>
    <path d="M 55 110 Q 65 105, 75 110" fill="none" stroke="#00a651" stroke-width="2" stroke-linecap="round"/>
    <path d="M 50 120 Q 60 115, 70 120" fill="none" stroke="#00a651" stroke-width="2" stroke-linecap="round"/>
  </g>

  <!-- CARB Text -->
  <text x="100" y="175" font-family="Arial, sans-serif" font-size="18" font-weight="bold" fill="#dc2626" text-anchor="middle">CARB</text>
</svg>
